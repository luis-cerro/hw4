<h1>Login</h1>

<%= form_with :url => "/sessions" do |form| %>
  <div class="mb-3">
  <p>
    <%= form.label "email" %>
    <%= form.text_field "email" %>
    <%= form.label "password" %>
    <%= form.password_field "password" %>
  </p>
  <p>
    <%= form.submit "Login", class: "btn btn-primary" %>
  </p>
  </div>
<% end %>